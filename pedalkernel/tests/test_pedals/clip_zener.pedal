# Zener diode clipping test circuit
# Asymmetric: forward Vf â‰ˆ 0.6V, reverse breakdown â‰ˆ 4.7V
# Creates distinctive asymmetric clipping character

pedal "Zener Clip Test" {
  components {
    C1: cap(100n)
    R1: resistor(4.7k)
    D1: zener(4.7)
    R2: resistor(10k)
  }
  nets {
    in -> C1.a
    C1.b -> R1.a
    R1.b -> D1.a, R2.a
    D1.b -> gnd
    R2.b -> out
  }
}
