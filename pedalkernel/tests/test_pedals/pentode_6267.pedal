# 6267 â€” American designation for EF86
# Should be identical to EF86 until individually modeled

pedal "Pentode 6267 Test" {
  supply 300V {
    impedance: 50
    filter_cap: 47u
    rectifier: solid_state
  }
  components {
    C1: cap(22n)
    R1: resistor(1M)
    V1: pentode(6267)
    R2: resistor(220k)
    R3: resistor(2.2k)
    C2: cap(25u)
    C3: cap(22n)
    R4: resistor(1M)
  }
  nets {
    in -> C1.a
    C1.b -> R1.a, V1.grid
    R1.b -> gnd
    vcc -> R2.a
    R2.b -> V1.plate
    V1.cathode -> R3.a, C2.a
    R3.b -> gnd
    C2.b -> gnd
    V1.plate -> C3.a
    C3.b -> R4.a
    R4.b -> gnd
    C3.b -> out
  }
}
